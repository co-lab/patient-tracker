{
  "name": "masaryk-patient-tracker-server",
  "version": "0.1.0",
  "description": "Backend of the system that manages patient data",
  "main": "src/index.js",
  "repository": "http://github.com/Masaryk/patient-tracker",
  "author": "Lincoln Clarete",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "nodemon src/index.js -x babel-node . -e js",
    "test": "eslint . && flow check $* && jest --coverage $*",
    "clean": "rm -rf coverage dist tmp",
    "dist": "babel src -d dist",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/node": "^7.0.0-beta.40",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0-beta.40",
    "@babel/plugin-transform-async-to-generator": "^7.0.0-beta.40",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.40",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.40",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.40",
    "@babel/polyfill": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "babel-core": "^7.0.0-beta.40",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "flow-bin": "^0.59.0",
    "flow-typed": "^2.3.0",
    "jest": "^22.4.2",
    "nodemon": "^1.15.1",
    "regenerator-runtime": "^0.11.1",
    "supertest": "^3.0.0"
  },
  "jest": {
    "testRegex": "tests/.*/*.js",
    "rootDir": "tests/",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "src/index.js"
    ],
    "collectCoverageFrom": [
      "src/*.js",
      "src/*/*.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules/.*",
      "helpers.js"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.40",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.1",
    "handlebars": "^4.0.11",
    "jsrsasign": "^8.0.6",
    "knex": "^0.14.4",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "nodemailer": "^4.5.0"
  }
}
